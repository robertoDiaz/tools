	st = status
	br = branch
	ri = rebase -i HEAD~10
	ri2 = rebase -i HEAD~20
	ri5 = rebase -i HEAD~50
	ri10 = rebase -i HEAD~100
	ca = commit --amend
	ra = rebase --abort
	rc = rebase --continue
	pum = pull upstream master
	pom = push origin master
	prum = pull --rebase upstream master
	hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
	type = cat-file -t
	dump = cat-file -p
	cb = checkout -b
	rm = branch -d
	bd = branch -D
